<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>it.contrader.servlets.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>it.contrader.servlets.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/LogoutServlet</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>4000</session-timeout>
  </session-config>
  <servlet>
    <description></description>
    <display-name>UserServlet</display-name>
    <servlet-name>UserServlet</servlet-name>
    <servlet-class>it.contrader.servlets.UserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserServlet</servlet-name>
    <url-pattern>/UserServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>MagazzinoServlet</servlet-name>
    <servlet-class>it.contrader.servlets.MagazzinoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MagazzinoServlet</servlet-name>
    <url-pattern>/MagazzinoServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>OggettoServlet</servlet-name>
    <servlet-class>it.contrader.servlets.OggettoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>OggettoServlet</servlet-name>
    <url-pattern>/OggettoServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AcquistoServlet</servlet-name>
    <servlet-class>it.contrader.servlets.AcquistoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AcquistoServlet</servlet-name>
    <url-pattern>/AcquistoServlet</url-pattern>
  </servlet-mapping>
  
</web-app>